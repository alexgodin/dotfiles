[alias]
  up = !git fetch origin && git rebase origin/master
  mm = !git checkout master && git merge @{-1} --ff-only
  st = status
  c  = commit -m
  aa = add .
  ci = commit -am
  br = branch
  co = checkout
  df = diff
  lg = log -p
  plre = pull --rebase
